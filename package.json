{
	"name": "zeeltephp",
	"version": "0.1.0.5",
	"description": "tbd",
	"license": "MIT",
	"trustedDependencies": ["zeeltephp"],
	"scripts": {
		"dev": "vite dev --mode dev",
		"build": "vite build --mode build",
		"preview": "vite preview",
		"prepare": "npm run svelte-kit sync || echo ''",
		"prepack.org": "npm run svelte-kit sync && npm run svelte-package && npm run cleanup && npm run zp-dist-copy-api && publint",
		"prepack": "npm run dist-create",
		"dist-create": "npm run svelte-kit sync && npm run svelte-package && npm run dist-copy-api && npm run dist-cleanup",
		"dist-cleanup": "npx del-cli ./dist/zplib && publint",
		"dist-copy-api": "npx cpy-cli 'static/api/**/*.{php,.htaccess}' dist/api ",
		"postinstall": "npm run dist-only && dist/vite-plugin/zp-post-install.js",
		"dist-only": "npx del-cli * !dist !package.json",
		"dist-bkp": " *.md *.config.js .svelte-kit .evn*"
	},
	"files": [
		"dist",
		"!dist/**/*.test.*",
		"!dist/**/*.spec.*"
	],
	"sideEffects": [
		"**/*.css"
	],
	"svelte": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"type": "module",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js"
		},
		"./vite-plugin": {
			"types": "./dist/vite-plugin/zeeltephp-vite-plugin.d.ts",
			"import": "./dist/vite-plugin/zeeltephp-vite-plugin.js"
		}
	},
	"exclude": ["src/lib/zplib"],
	"peerDependencies": {
		"svelte": "^5.0.0",
		"@sveltejs/adapter-static": "^3.0.8",
		"@sveltejs/kit": "^2.20.7"
	},
	"devDependencies": {
		"@picocss/pico": "^2.1.1",
		"@sveltejs/vite-plugin-svelte": "^5.0.3",
		"@sveltejs/package": "^2.3.11",
		"svelte": "^5.28.2",
		"typescript": "^5.8.3",
		"vite": "^6.3.4",
		"publint": "^0.3.12"
	},
	"keywords": [
		"svelte"
	]
}
