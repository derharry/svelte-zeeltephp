{
	"extends": "./.svelte-kit/tsconfig.json",
	"compilerOptions": {
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"resolveJsonModule": true,
		"skipLibCheck": true,
		"sourceMap": true,
		"module": "NodeNext",
		"moduleResolution": "NodeNext"
	},
	"exclude": [
	  "src/lib/templates/**",
	  "src/lib/zeeltephp/**",
	  "src/lib/zplib/**",
	  "node_modules/**"
	]
}
